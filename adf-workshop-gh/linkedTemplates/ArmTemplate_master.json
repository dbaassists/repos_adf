{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-workshop-gh"
		},
		"ls_adls_xpto_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_xpto'"
		},
		"ls_asql_xpto_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_asql_xpto'"
		},
		"ls_adls_xpto_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsworkshopbiazure.dfs.core.windows.net/"
		},
		"ls_asql_xpto_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "asql-database-workshop.database.windows.net"
		},
		"ls_asql_xpto_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "asql-database-workshop"
		},
		"ls_asql_xpto_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "uasqlworkshop"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-workshop-gh_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_xpto_accountKey": {
						"value": "[parameters('ls_adls_xpto_accountKey')]"
					},
					"ls_asql_xpto_password": {
						"value": "[parameters('ls_asql_xpto_password')]"
					},
					"ls_adls_xpto_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_xpto_properties_typeProperties_url')]"
					},
					"ls_asql_xpto_properties_typeProperties_server": {
						"value": "[parameters('ls_asql_xpto_properties_typeProperties_server')]"
					},
					"ls_asql_xpto_properties_typeProperties_database": {
						"value": "[parameters('ls_asql_xpto_properties_typeProperties_database')]"
					},
					"ls_asql_xpto_properties_typeProperties_userName": {
						"value": "[parameters('ls_asql_xpto_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}